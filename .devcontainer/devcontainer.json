{
  "name": "unremarkable-ideaogram-assistant",
  "build": {
    "dockerfile": "Dockerfile",
    "context": ".",
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "esbenp.prettier-vscode"
      ],
      "settings": {
        "[javascript]": {
          "editor.formatOnSave": true,
          "editor.defaultFormatter": "esbenp.prettier-vscode"
        }
      }
    }
  },
  "features": {
    "ghcr.io/devcontainers/features/common-utils:2": {},
  },
  "remoteUser": "vscode",
}
